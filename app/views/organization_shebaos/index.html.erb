<p id="notice"><%= notice %></p>

<h1>Listing Organization Shebaos</h1>

<table>
  <thead>
    <tr>
      <th>Organization customer</th>
      <th>Account no</th>
      <th>Workflow state</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organization_shebaos.each do |organization_shebao| %>
      <tr>
        <td><%= organization_shebao.organization_customer_id %></td>
        <td><%= organization_shebao.account_no %></td>
        <td><%= organization_shebao.workflow_state %></td>
        <td><%= link_to 'Show', organization_shebao %></td>
        <td><%= link_to 'Edit', edit_organization_shebao_path(organization_shebao) %></td>
        <td><%= link_to 'Destroy', organization_shebao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

