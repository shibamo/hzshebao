<h1 class="text-center">社保应缴金额列表</h1>

<table class="table table-striped table-hover table-condensed">
  <thead>
    <tr>
      <th>年份</th>
      <th>社保应缴金额</th>
      <th>设置人</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @shebao_bases.each do |shebao_base| %>
      <tr>
        <td><%= shebao_base.year %></td>      
        <td><%= shebao_base.base %></td>
        <td><%= shebao_base.user.name %></td>
        <td><%= link_to '编辑', edit_shebao_basis_path(shebao_base), class: "btn btn-default" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '新建', new_shebao_basis_path, class: "btn btn-default" %>
