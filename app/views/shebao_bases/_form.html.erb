<%= simple_form_for(@shebao_base) do |f| %>

  <div class="form-inputs">
    <%= f.input :year, label: "年" , input_html: { class: 'text-right' }%>
    <%= f.input :base, label: "社保应缴金额" , input_html: { class: 'text-right' }%>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "提交",class: "btn-primary" %>
    <%= link_to '返回', shebao_bases_path, class: "btn btn-default"  %>
  </div>
<% end %>
