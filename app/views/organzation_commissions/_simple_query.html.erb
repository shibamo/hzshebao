<%= simple_form_for @organzation_commission ||= @model_class.new, :url => url, :method => :post, html: {class: 'form-inline' } do |f| %>
  <div class="form-inputs">
  客户姓名
  <input class="string optional form-control" size="8" type="text" 
                value="<%= @organzation_customer_name ||= nil %>" name="organzation_commission[organzation_customer_name]">&nbsp;
    资金到账日期从:
      <input class="string optional form-control dtp" size="10" type="text" value="<%=  @input_date_from ||= nil %>" id="dp1" data-date-format="yyyy-mm-dd" name="organzation_commission[input_date_from]">
      <span class="add-on"><i class="icon-th"></i></span>
    到:    
      <input class="string optional form-control dtp" size="10" type="text" value="<%=  @input_date_to ||= nil %>" id="dp2" data-date-format="yyyy-mm-dd"  name="organzation_commission[input_date_to]">
      <span class="add-on"><i class="icon-th"></i></span>&nbsp;&nbsp;
    <%= f.button :submit,"查询" , class: "btn btn-primary"  %>
  </div><br>
<%end%>
<script type="text/javascript">
$('.dtp').datepicker();
</script>