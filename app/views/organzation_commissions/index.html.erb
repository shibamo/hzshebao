<p id="notice"><%= notice %></p>

<h1>Listing Organzation Commissions</h1>

<table>
  <thead>
    <tr>
      <th>Commission no</th>
      <th>Organization charge total</th>
      <th>User</th>
      <th>Bonus reference</th>
      <th>Bonus</th>
      <th>Approver</th>
      <th>Financer</th>
      <th>Workflow state</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organzation_commissions.each do |organzation_commission| %>
      <tr>
        <td><%= organzation_commission.commission_no %></td>
        <td><%= organzation_commission.organization_charge_total_id %></td>
        <td><%= organzation_commission.user_id %></td>
        <td><%= organzation_commission.bonus_reference %></td>
        <td><%= organzation_commission.bonus %></td>
        <td><%= organzation_commission.approver_id %></td>
        <td><%= organzation_commission.financer_id %></td>
        <td><%= organzation_commission.workflow_state %></td>
        <td><%= link_to 'Show', organzation_commission %></td>
        <td><%= link_to 'Edit', edit_organzation_commission_path(organzation_commission) %></td>
        <td><%= link_to 'Destroy', organzation_commission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organzation commission', new_organzation_commission_path %>
