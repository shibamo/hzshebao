<%= simple_form_for(@organization_charge_other) do |f| %>
  <% if @organization_charge_other.errors.any? %>
    <div id="error_explanation">
      <h2>以下错误发生,无法保存: </h2>
      <ul>
      <%@organization_charge_other.errors.messages.each do |message|
        message[1].each do |err_msg|%>
          <li> <%=err_msg%> </li>
        <% end %>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="form-inputs">
    <%= f.input :organization_id, as: :hidden %>
    <div class="form-group  col-xs-2 required ">
      <%= f.label "服务开始日期" %>
      <%= f.text_field :start_date, :class => "dtp form-control required", "data-date-format" => "yyyy-mm-dd" %>
    </div>
    <div class="form-group  col-xs-2 required ">
      <%= f.label "服务结束日期" %>
      <%= f.text_field :end_date, :class => "dtp form-control required", "data-date-format" => "yyyy-mm-dd" %>
    </div>
    <div class="form-group  col-xs-2 required ">    
      <%= f.input :price_fuwufei, label: "服务费" %>
    </div>
    <div class="form-group  col-xs-2 required ">
      <%= f.input :price_canbao, label: "残保金" %>
    </div>
    <div class="form-group  col-xs-2 required ">
      <%= f.input :price_chajia, label: "差价" %>
    </div>
    <div class="form-group  col-xs-2 required ">
    <%= f.input :price_gonghui, label: "工会费" %>
    </div>
    <div class="form-group  col-xs-2 required ">
    <%= f.input :price_qita_1, label: "其它1" %>
    </div>
    <div class="form-group  col-xs-2 required ">
    <%= f.input :price_qita_2, label: "其它2" %>
    </div>
    <div class="form-group  col-xs-2 required ">
    <%= f.input :price_qita_3, label: "其它3" %>
    </div>
    <div class="form-group  col-xs-12 required ">
    <%= f.input :comment , as: :text, label: "备注" %>
    </div>

    <div class="form-actions col-xs-12">
      <%= f.submit("提 交", class:"actions btn btn-primary noprint") unless read_mode %>
      <%= link_to '返回', organization_charge_others_list_by_organization_path(@organization) , class: "btn btn-default" %>
    </div>
  </div>
<% end %>

<script type="text/javascript">
  $('.dtp').datepicker();
</script>