<h1>机构(<%=@organization.name%>)缴费历史记录</h1>

<table>
  <thead>
    <tr>
      <th>Price shebao base</th>
      <th>Price shebao qiye</th>
      <th>Price shebao geren</th>
      <th>Price canbao</th>
      <th>Price shebao guanli</th>
      <th>Price gongjijin base</th>
      <th>Price gongjijin qiye</th>
      <th>Price gongjijin geren</th>
      <th>Price gongjijin guanli</th>
      <th>Price geshui</th>
      <th>Price qita 1</th>
      <th>Price qita 2</th>
      <th>Price qita 3</th>
      <th>Price bujiao</th>
      <th>Price yujiao</th>
      <th>Price gongzi</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Comment</th>
      <th>Money arrival date</th>
      <th>Money check date</th>
      <th>Workflow state</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organization_charge_totals.each do |organization_charge_total| %>
      <tr>
        <td><%= organization_charge_total.price_shebao_base %></td>
        <td><%= organization_charge_total.price_shebao_qiye %></td>
        <td><%= organization_charge_total.price_shebao_geren %></td>
        <td><%= organization_charge_total.price_canbao %></td>
        <td><%= organization_charge_total.price_shebao_guanli %></td>
        <td><%= organization_charge_total.price_gongjijin_base %></td>
        <td><%= organization_charge_total.price_gongjijin_qiye %></td>
        <td><%= organization_charge_total.price_gongjijin_geren %></td>
        <td><%= organization_charge_total.price_gongjijin_guanli %></td>
        <td><%= organization_charge_total.price_geshui %></td>
        <td><%= organization_charge_total.price_qita_1 %></td>
        <td><%= organization_charge_total.price_qita_2 %></td>
        <td><%= organization_charge_total.price_qita_3 %></td>
        <td><%= organization_charge_total.price_bujiao %></td>
        <td><%= organization_charge_total.price_yujiao %></td>
        <td><%= organization_charge_total.price_gongzi %></td>
        <td><%= organization_charge_total.start_date %></td>
        <td><%= organization_charge_total.end_date %></td>
        <td><%= organization_charge_total.comment %></td>
        <td><%= organization_charge_total.money_arrival_date %></td>
        <td><%= organization_charge_total.money_check_date %></td>
        <td><%= organization_charge_total.workflow_state %></td>
        <td><%= link_to 'Show', organization_charge_total %></td>
        <td><%= link_to 'Edit', edit_organization_charge_total_path(organization_charge_total) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organization charge total', organization_charge_totals_new_path(@organization) %>
