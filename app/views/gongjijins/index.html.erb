<p id="notice"><%= notice %></p>

<h1>Listing Gongjijins</h1>

<table>
  <thead>
    <tr>
      <th>Single customer</th>
      <th>Account no</th>
      <th>Workflow state</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gongjijins.each do |gongjijin| %>
      <tr>
        <td><%= gongjijin.single_customer_id %></td>
        <td><%= gongjijin.account_no %></td>
        <td><%= gongjijin.workflow_state %></td>
        <td><%= link_to 'Show', gongjijin %></td>
        <td><%= link_to 'Edit', edit_gongjijin_path(gongjijin) %></td>
        <td><%= link_to 'Destroy', gongjijin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gongjijin', new_gongjijin_path %>
