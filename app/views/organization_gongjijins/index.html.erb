<p id="notice"><%= notice %></p>

<h1>Listing Organization Gongjijins</h1>

<table>
  <thead>
    <tr>
      <th>Organization customer</th>
      <th>Account no</th>
      <th>Workflow state</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organization_gongjijins.each do |organization_gongjijin| %>
      <tr>
        <td><%= organization_gongjijin.organization_customer_id %></td>
        <td><%= organization_gongjijin.account_no %></td>
        <td><%= organization_gongjijin.workflow_state %></td>
        <td><%= link_to 'Show', organization_gongjijin %></td>
        <td><%= link_to 'Edit', edit_organization_gongjijin_path(organization_gongjijin) %></td>
        <td><%= link_to 'Destroy', organization_gongjijin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

